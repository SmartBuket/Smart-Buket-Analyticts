{
  "uid": "sb-apis",
  "title": "SmartBuket APIs (Prometheus)",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "10s",
  "panels": [
    {
      "type": "timeseries",
      "title": "HTTP req/s by service",
      "id": 1,
      "gridPos": {"x": 0, "y": 0, "w": 12, "h": 8},
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(sb_http_requests_total[5m])) by (service)",
          "legendFormat": "{{service}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "p95 latency (s) by service",
      "id": 2,
      "gridPos": {"x": 12, "y": 0, "w": 12, "h": 8},
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.95, sum(rate(sb_http_request_duration_seconds_bucket[5m])) by (le, service))",
          "legendFormat": "{{service}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Rate-limited req/s by service",
      "id": 3,
      "gridPos": {"x": 0, "y": 8, "w": 12, "h": 8},
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(sb_rate_limited_total[5m])) by (service)",
          "legendFormat": "{{service}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "HTTP req/s by status",
      "id": 4,
      "gridPos": {"x": 12, "y": 8, "w": 12, "h": 8},
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(sb_http_requests_total[5m])) by (service, status)",
          "legendFormat": "{{service}} {{status}}"
        }
      ]
    }
  ]
}
